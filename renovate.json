{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:recommended"
    ],
    "packageRules": [
      {
        "matchManagers": ["regex"],
        "matchPackageNames": ["cpp-linter/clang-tools-static-binaries"],
        "extractVersion": "^master-\\w*"
      }
    ],
    "customManagers": [
      {
        "customType": "regex",
        "fileMatch": ["clang_tools/install.py", "tests/test_util.py"],
        "matchStrings": ["^master-\\w*"]
      }
    ]
  }
